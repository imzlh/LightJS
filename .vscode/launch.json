    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ljs",
            "preLaunchTask": "CMake: 生成",
            // "args": ["--memory-limit=64m", "example/tinyhttp", "http://localhost:8000"],
            "args": ["--memory-limit=64m", "test", "process"],
            // "args": ["--memory-limit=64m", "src/repl.js"],
            // "args": ["--memory-limit=64m", "${file}"],
            // "args": ["--memory-limit=64m", "lib/lbundler.js", "lib"],
            // "args": ["--memory-limit=64m", "-e", "import 'process'"],
            // "args": ["--memory-limit=64m", "test.js"],
            // "args": ["--memory-limit=64m", "lib/tsrt", "example/webtty"],
            // "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },{
            "name": "(gdb) 启动ljsc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ljsc",
            "preLaunchTask": "CMake: 生成",
            // "args": [],
            "args": ["--memory-limit=64m"],
            // "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },{
            "name": "(gdb, cygwin) 启动ljs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ljs.exe",
            "args": ["--memory-limit=64m", "test", "worker"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "将反汇编风格设置为 Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}